#include<stdio.h>
#include<math.h> // power nikalna ko lagi ,, natra 2x2 , 2x2x2 , 2x2x2 garna parthyo
                // pow use garya pachi multiply garna parena .
int main(){
    // expected output is 2^1 = 2 , 2^2 = 4 , 2^3 = 8
    int i,num = 2;
    for ( i = 1; i <= 3 ; i++)// loop chalayera power ma rakhdini.
    {   // hya maths.h lay int return na gari double wala value return garxa
        // double vanako long float ho ,,so , %lf ,, yedi %lf ko satta %d lekhyo vanya (int)pow(num,i) lekhna parcha.
        printf(" the first 3 powers of %d is : %lf \n ",num,pow(num,i));
    }
    

    return 0;
}
