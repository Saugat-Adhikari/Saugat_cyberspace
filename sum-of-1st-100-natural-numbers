#include<stdio.h>
int main(){
    int i,sum=0;
    for ( i = 1; i <= 100; i++)
    {
        sum = sum + i; //sum update hudai janxa 
    }
    printf(" the sum of first 100 number is : %d ",sum); // yo loop baira vayako karan lay calculation ko last value matra print hunca. which is 5050
    

       return 0;
}
/*
sum is 0 in initial
sum+i
0+1 = 1
1+2 = 2
2+3 = 5
5+4 = 9
.
. ==5050 
*/

